<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/yjpark/dap.lib.fsharp</PackageProjectUrl>
    <PackageTags>fsharp;dap;</PackageTags>
    <Authors>YJ Park</Authors>
    <Version>0.6.0</Version>
    <Description>Dap with Xamarin.Forms</Description>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Util.fs" />
    <Compile Include="Dsl.fs" />
    <Compile Include="_Gen/Types.fs" />
    <Compile Include="_Gen/Builder/Types.fs" />
    <Compile Include="Runner.fs" />
    <Compile Include="Provider/FileSystem.fs" />
    <Compile Include="Provider/Preferences.fs" />
    <Compile Include="Provider/SecureStorage.fs" />
    <Compile Include="View/Types.fs" />
    <Compile Include="View/Logic.fs" />
    <Compile Include="Meta.fs" />
    <Compile Include="Helper.fs" />
    <Compile Include="Builder/Helper.fs" />
    <Compile Include="Prefab/Label.fs" />
    <Compile Include="Prefab/Button.fs" />
    <Compile Include="Prefab/TextField.fs" />
    <Compile Include="Prefab/Stack.fs" />
    <Compile Include="Generator/Gui.fs" />
    <Compile Include="Generator/Helper.fs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta-62925-02" PrivateAssets="All" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>